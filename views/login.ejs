
<% include partials/header %>
<link rel="stylesheet" href="stylesheets/register-style.css">
<link rel="stylesheet" href="stylesheets/login-style.css">
</head>
<body id="body-background">
<div class="upper-box"></div>
<div class="container main-box">
    <form class="col 12" action="/login" method="POST">
        <div class="row">
            <div class="col s2"></div>
            <div class="input-field col s7">
                <h5>Username</h5>
                <input id="first_name" type="text" class="validate" name="username">
            </div>
            <div class="col s1"></div>
        </div>
        <div class="row">
            <div class="col s2"></div>
            <div class="input-field col s7">
                <h5>Password</h5>
                <input id="password" type="password" class="validate" name="password">
            </div>
        </div>
        <div class="row">
            <div class="col s2"></div>
            <div class="col s7">
                <button class="waves-effect red darken-3 btn-large">LogIn</button>
            </div>
            <div class="col s2"></div>
        </div>
    </form>
    <br>
</div>
</body>